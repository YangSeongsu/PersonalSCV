<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context="com.scv.commendcenter.map_sample.MainActivity"
    android:id="@+id/main_map"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    >

    <LinearLayout
        android:id="@+id/first_set"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:orientation="horizontal"
        >
        <TextView
            android:id="@+id/area_01"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="#555555"
            android:text="유가공품"
            android:textSize="10pt"
            android:gravity="center_horizontal|center_vertical"
            />

        <TextView
            android:id="@+id/area_02"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="#FF0000"
            android:text="육류"
            android:textSize="10pt"
            android:gravity="center_horizontal|center_vertical"
            />

        <TextView
            android:id="@+id/area_03"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="#00FF00"
            android:text="생선"
            android:textSize="10pt"
            android:gravity="center_horizontal|center_vertical"
            />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/second_set"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="5"
        android:orientation="horizontal"
        >

        <LinearLayout
            android:id="@+id/sellBar_01"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="2"
            android:orientation="vertical"
            >

            <TextView
                android:id="@+id/area_04"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:background="#0000FF"
                android:text="음료"
                android:textSize="10pt"
                android:gravity="center_horizontal|center_vertical"
                />

            <TextView
                android:id="@+id/area_08"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:background="#00FF00"
                android:text="주류"
                android:textSize="10pt"
                android:gravity="center_horizontal|center_vertical"
                />

            <TextView
                android:id="@+id/area_12"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:background="#555555"
                android:text="잡화"
                android:textSize="10pt"
                android:gravity="center_horizontal|center_vertical"
                />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/Load_1"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="vertical" />

        <LinearLayout
            android:id="@+id/sellBar_02"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="4"
            android:orientation="vertical"
            >

            <LinearLayout
                android:id="@+id/Load_2"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:orientation="vertical" />

            <LinearLayout
                android:id="@+id/sellBar_04"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="2"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/area_05"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="#555555"
                    android:text="스낵"
                    android:textSize="10pt"
                    android:gravity="center_horizontal|center_vertical"
                    />

                <TextView
                    android:id="@+id/area_06"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="#00FF00"
                    android:text="라면"
                    android:textSize="10pt"
                    android:gravity="center_horizontal|center_vertical"
                    />

            </LinearLayout>



            <LinearLayout
                android:id="@+id/Load_3"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="2"
                android:orientation="vertical" />

            <LinearLayout
                android:id="@+id/sellBar_05"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="2"
                android:orientation="horizontal">

                <TextView
                android:id="@+id/area_09"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="#555555"
                android:text="소스"
                android:textSize="10pt"
                android:gravity="center_horizontal|center_vertical"
                />

                <TextView
                    android:id="@+id/area_10"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="#00FF00"
                    android:text="통조림"
                    android:textSize="10pt"
                    android:gravity="center_horizontal|center_vertical"
                    />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/Load_4"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="2"
                android:orientation="vertical" />

            <LinearLayout
                android:id="@+id/sellBar_06"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="2"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/area_13"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="#555555"
                    android:text="과일"
                    android:textSize="10pt"
                    android:gravity="center_horizontal|center_vertical"
                    />

                <TextView
                    android:id="@+id/area_14"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="#00FF00"
                    android:text="야채"
                    android:textSize="10pt"
                    android:gravity="center_horizontal|center_vertical"
                    />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/Load_5"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:orientation="vertical" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/Load_6"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="vertical" />

        <LinearLayout
            android:id="@+id/sellBar_03"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="2"
            android:orientation="vertical"
            >

            <TextView
                android:id="@+id/area_07"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:background="#0000FF"
                android:text="낙농"
                android:textSize="10pt"
                android:gravity="center_horizontal|center_vertical"
                />

            <TextView
                android:id="@+id/area_11"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:background="#555555"
                android:text="유아식"
                android:textSize="10pt"
                android:gravity="center_horizontal|center_vertical"
                />

            <TextView
                android:id="@+id/area_15"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:background="#00ff00"
                android:text="생필품"
                android:textSize="10pt"
                android:gravity="center_horizontal|center_vertical"
                />

        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/third_set"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/area_16"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="3"
            android:background="#00FF00"
            android:text="베이커리"
            android:textSize="10pt"
            android:gravity="center_horizontal|center_vertical"
            />

        <TextView
            android:id="@+id/area_17"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="2"
            android:background="#FF0000"
            android:text="입구"
            android:textSize="10pt"
            android:gravity="center_horizontal|center_vertical"
            />

    </LinearLayout>

</LinearLayout>
